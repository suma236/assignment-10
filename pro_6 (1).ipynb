      "source": [
        "def up_low(string):\n",
        "    uppers=0\n",
        "    lowers=0\n",
        "    for char in string:\n",
        "        if char.islower():\n",
        "            lowers+=1\n",
        "        elif char.isupper():\n",
        "            uppers+=1\n",
        "        else:\n",
        "            pass\n",
        "    return(uppers,lowers)\n",
        "print(up_low(\"hello mr.rogers,how are you this fine tuesday?\"))"
      ],
